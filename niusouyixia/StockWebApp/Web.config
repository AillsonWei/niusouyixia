<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=5.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
  </configSections>
  <connectionStrings>
    <add name="db_StockConnectionString" connectionString="Data Source=CY-20140724AJHV;Initial Catalog=db_Stock;Integrated Security=True" providerName="System.Data.SqlClient"/>
    <add name="db_StockConnectionString2" connectionString="Data Source=CY-20140724AJHV;Initial Catalog=db_Stock;Integrated Security=True" providerName="System.Data.SqlClient"/>
